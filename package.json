{
  "name": "graphql-auth",
  "license": "MIT",
  "main": "dist/server",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "start": "node dist/server",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "generate": "prisma2 generate && yarn generate:nexus",
    "generate:prisma": "",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "postinstall": "test -n \"$NOYARNPOSTINSTALL\" || npm -s run generate",
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly src/server",
    "seed": "ts-node prisma/seed",
    "seed:airtable": "ts-node prisma/airtable",
    "prestart": "echo \"DATABASE_URL=${DATABASE_URL}\" > .env"
  },
  "dependencies": {
    "@barajs/formula": "3.0.0-alpha.481",
    "@prisma/client": "2.0.0-preview021",
    "@prisma/sdk": "0.0.105",
    "@types/p-limit": "2.2.0",
    "airtable": "0.8.1",
    "axios": "0.19.0",
    "bcryptjs": "2.4.3",
    "dotenv": "8.2.0",
    "google-libphonenumber": "3.2.6",
    "graphql-shield": "^7.0.9",
    "graphql-yoga": "^1.18.3",
    "jsonwebtoken": "8.5.1",
    "moment": "2.24.0",
    "nexus": "0.12.0-rc.9",
    "nexus-prisma": "0.9.0",
    "p-limit": "2.2.2",
    "p-retry": "4.2.0",
    "ts-node": "8.5.4",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript": "3.7.4",
    "vikit": "0.3.4"
  },
  "devDependencies": {
    "@types/airtable": "0.5.7",
    "@types/bcryptjs": "2.4.2",
    "@types/dotenv": "8.2.0",
    "@types/google-libphonenumber": "7.4.17",
    "@types/jsonwebtoken": "8.3.5",
    "@types/node": "12.12.22",
    "@types/p-retry": "3.0.1",
    "@types/ws": "6.0.4",
    "prisma2": "2.0.0-preview021"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  }
}
